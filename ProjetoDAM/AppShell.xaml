<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProjetoDAM.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProjetoDAM"
    Shell.FlyoutBehavior="Disabled"
    Title="ProjetoDAM"
    FlyoutBackgroundColor="#FCFAEE"
    >

    <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        ContentTemplate="{DataTemplate local:AddPhotosPage}"
        Route="AddPhoto" />

</Shell>
